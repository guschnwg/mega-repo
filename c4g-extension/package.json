{
	"name": "c4g-extension",
	"displayName": "c4g-extension",
	"description": "",
	"version": "0.0.7",
	"publisher": "GiovannaZanardini",
	"license": "MIT",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.runTestsFile",
				"title": "Run tests file"
			},
			{
				"command": "extension.runTestMethod",
				"title": "Run"
			},
			{
				"command": "extension.copyImportStatement",
				"title": "Copy import statement"
			},
			{
				"command": "extension.openConsoleWithThisImported",
				"title": "Open console"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"ship": "vsce package --no-dependencies"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"vsce": "^2.13.0"
	}
}